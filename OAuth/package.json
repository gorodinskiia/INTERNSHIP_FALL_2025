{
  "name": "oauth-2-example",
  "version": "1.0.0",
  "description": "Comprehensive OAuth 2.0 implementation example with multiple providers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint server.js",
    "docker:build": "docker build -t oauth-example .",
    "docker:run": "docker run -p 3000:3000 --env-file .env oauth-example",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "oauth2",
    "authentication",
    "google-oauth",
    "github-oauth",
    "passport",
    "express",
    "security"
  ],
  "author": "OAuth Example",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-github2": "^0.1.12",
    "express-session": "^1.17.3",
    "axios": "^0.21.4",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/oauth-example.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/oauth-example/issues"
  },
  "homepage": "https://github.com/your-username/oauth-example#readme"
}
